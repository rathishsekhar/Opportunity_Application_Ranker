    "    \n",
    "    for column in [m for m in job_column if m in str_column]:\n",
    "        text = llm_job_data_bert.at[index, column + \"__bertpp\"]\n",
    "        sentences = nltk.sent_tokenize(text)\n",
    "        \n",
    "        input = tokenizer(\n",
    "            sentences, padding = True, \n",
